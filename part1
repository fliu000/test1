w = 0.0005;
n = 10;
function test1 = P(n)
    for i = 1 : (n - 1)
        P1 = 2 / 3 * (c((n - i) * w) - c((n - i - 1) * w)) * ((i + 1)^(3/2) - i^(3/2)) * sqrt(w);
        P2 = 2 * ((i + 1) * c((n - i) * w) - i * c((n - i - 1) * w)) * ((i + 1)^(1/2) - i^(1/2)) * sqrt(w);
    end
end
  disp(P1);
  disp(P2);
  
  
 function result = c(num)
    result = 2 * sqrt (30 * num / pi);
 end      
